  "compilerOptions": {
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true, 
    "strict": true, 
    "module": "ESNext",
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true
  }
}
